<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/plusadmin_theme/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/plusadmin_theme/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/plusadmin_theme/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/plusadmin_theme/vendors/jquery-bar-rating/css-stars.css" />
    <link rel="stylesheet" href="~/plusadmin_theme/vendors/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/lib/fontawesome6/css/all.css" />
    <link rel="stylesheet" href="~/plusadmin_theme/css/theme_1/style.css" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

</head>
<body>
    <div class="base-container-scroller">
        <main role="main" class="pb-3" style="padding: 0px !important">
            @RenderBody()
        </main>
    </div>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/plusadmin_theme/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/plusadmin_theme/vendors/jquery-bar-rating/jquery.barrating.min.js"></script>
    <script src="~/plusadmin_theme/vendors/chart.js/Chart.min.js"></script>
    <script src="~/plusadmin_theme/vendors/flot/jquery.flot.js"></script>
    <script src="~/plusadmin_theme/vendors/flot/jquery.flot.resize.js"></script>
    <script src="~/plusadmin_theme/vendors/flot/jquery.flot.categories.js"></script>
    <script src="~/plusadmin_theme/vendors/flot/jquery.flot.fillbetween.js"></script>
    <script src="~/plusadmin_theme/vendors/flot/jquery.flot.stack.js"></script>
    <script src="~/plusadmin_theme/js/off-canvas.js"></script>
    <script src="~/plusadmin_theme/js/hoverable-collapse.js"></script>
    <script src="~/plusadmin_theme/js/misc.js"></script>
    <script src="~/plusadmin_theme/js/settings.js"></script>
    <script src="~/plusadmin_theme/js/todolist.js"></script>
    <script src="~/plusadmin_theme/js/dashboard.js"></script>
    <script src="~/js/side-menu.js" asp-append-version="true"></script>
    <script src="~/lib/fontawesome6/js/all.js"></script>
    @*SweetAlert*@
    <script src="~/lib/sweetalert/sweetalert.min.js"></script>
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert.min.css" />
    @*Chosen*@
    <script src="~/lib/chosen/chosen.jquery.js"></script>
    <script src="~/lib/chosen/docsupport/prism.js"></script>
    <script src="~/lib/chosen/docsupport/init.js"></script>
    @*<script src="~/lib/canvasjs/jquery.canvasjs.min.js"></script>*@
    <script src="~/lib/canvasjs/canvasjs.min.js"></script>
    <link rel="stylesheet" href="~/lib/chosen/chosen.css" />
    @*Datatable*@
    <script type="text/javascript" src="~/lib/datatable/datatables.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/lib/datatable/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/buttons/2.2.3/css/buttons.dataTables.min.css" />
    
    @*FileInput*@
    <script src="~/lib/fileinput/fileinput.min.js"></script>
    <link href="~/lib/fileinput/fileinput.min.css" media="all" rel="stylesheet" type="text/css" />

    <script src="~/lib/fileinput/fas/theme.js"></script>
    <script src="~/lib/fileinput/plugins/piexif.min.js" type="text/javascript"></script>
    <script src="~/lib/fileinput/plugins/sortable.min.js" type="text/javascript"></script>
    @*ZingChart*@
    <script src="~/lib/zingchart/zingchart.min.js"></script>
    @*LoadingOverlay*@
    <script src="~/lib/loading-overlay/loadingoverlay.min.js"></script>
    
    @*DraggableJs*@
    <script src="~/lib/draggablejs/draggable.min.js"></script>

    @*PackeryJs*@
    <script src="~/lib/packeryjs/packery.min.js"></script>

    @*Toast*@
    <link rel="stylesheet" href="~/lib/toast/toast.css">
    <script type="text/javascript" src="~/lib/toast/toast.js"></script>

    @*Query Builder*@
    <link href="~/lib/jquery-query-builder/css/query-builder.min.css" media="all" rel="stylesheet" type="text/css" />
    <script src="~/lib/jquery-query-builder/js/query-builder.min.js"></script>

    @*amcharts*@
    <script src="https://cdn.amcharts.com/lib/4/core.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/charts.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/geodata/worldHigh.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/themes/dark.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/maps.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/themes/material.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/themes/animated.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/plugins/timeline.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/plugins/bullets.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/map.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/geodata/worldLow.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/index.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/xy.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/radar.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/themes/Animated.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/percent.js"></script>
    @*datatable tools + export*@
    <script src='//cdn.datatables.net/buttons/1.6.2/js/dataTables.buttons.min.js'></script>
    <script src='//cdn.datatables.net/buttons/1.6.2/js/buttons.flash.min.js'></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src= "//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="//cdn.datatables.net/buttons/1.6.2/js/buttons.html5.min.js"></script>
    <script src= "//cdn.datatables.net/buttons/1.6.2/js/buttons.print.min.js"></script>
    <script src= "//cdn.datatables.net/buttons/2.2.3/js/buttons.colVis.min.js"></script>

    
    @*datatable col resize*@
    <link href="~/lib/dataTables-colResize/jquery.dataTables.colResize.css" rel="stylesheet" type="text/css" />
    <script src="~/lib/dataTables-colResize/jquery.dataTables.colResize.js"></script>

    <script src='https://npmcdn.com/@@turf/turf/turf.min.js'></script>

    @* datatable *@
    <link href="~\lib\datatable\datatables.min.css" rel="stylesheet" type="text/css"/>
    <script src="~\lib\datatable\datatables.min.js"></script>

</body>
</html>
